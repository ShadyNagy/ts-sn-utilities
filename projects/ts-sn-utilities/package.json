{
  "name": "ts-sn-utilities",
  "author": "Shady Nagy <info@shadynagy.com>",
  "version": "0.1.4",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "description": "A variant of RxJS exhaustMap that includes the trailing value emitted from the source observable while waiting for the inner observable to complete",
  "scripts": {
    "test": "jest",
    "build": "run-p build:*",
    "build:cjs": "tsc -p tsconfig.build.json --module commonjs --outDir dist/cjs",
    "build:esm": "tsc -p tsconfig.build.json --module es2020 --outDir dist/esm",
    "prepublishOnly": "npm run build",
    "release": "standard-version"
  },
  "keywords": [
    "Rx",
    "RxJS",
    "ReactiveX",
    "ReactiveExtensions",
    "Streams",
    "Observables",
    "Observable",
    "Stream",
    "ES6",
    "ES2015",
    "ts",
    "typescript",
    "angular",
    "utilities",
    "framework",
    "frontend"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rxjs": "^6.5.4",
    "standard-version": "^9.2.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "rxjs": "6.x"
  },
  "homepage": "https://shadynagy.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShadyNagy/ts-sn-utilities.git"
  },
  "bugs": {
    "url": "https://github.com/ShadyNagy/ts-sn-utilities/issues"
  }
